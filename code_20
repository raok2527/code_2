#include<stdio.h>
void main()
{
  int num[]={24,34,12,44,56,17};
 
    int i=1;
    while(i<=6)
    {
      printf("\n element no is %d",num[i]);
      printf("\n address is %u",&num[i]);
      printf("\n %d",*(num+i));
      printf("\n %d",*(i+num));
      printf("\n %d",i[num]);
      i++;
      
    }

  
}
